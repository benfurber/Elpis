{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015"],
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "app": ["app"],
      "assets/*": ["app/assets/*"],
      "components": ["app/components"],
      "content": ["app/content"],
      "factories": ["app/factories"],
      "interfaces": ["app/interfaces"],
      "labels": ["app/labels"],
      "mutations": ["app/mutations"],
      "queries": ["app/queries"],
      "routes": ["app/routes"],
      "screens": ["app/screens"],
      "styles": ["app/styles"],
      "utils": ["app/utils"]
    },
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "esModuleInterop": true
  }
}
