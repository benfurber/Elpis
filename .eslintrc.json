{
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["airbnb", "eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "react", "react-native"],
  "rules": {
    "import/prefer-default-export": false,
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [0]
  }
}
